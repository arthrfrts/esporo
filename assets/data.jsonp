---
layout: none
---
tags([
  {%- for tag in site.tags -%}
    {
      "value": {{ tag | first | jsonify }}
    }{%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
]);

posts([
  {% for post in site.posts %}
    {
      "text": {{ post.title | jsonify }},
      "href": {{ post.url | absolute_url | jsonify }}
    }{%- unless forloop.last -%},{%- endunless -%}
  {% endfor %}
]);
