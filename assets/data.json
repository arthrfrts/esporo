---
layout: null
---
{
  "tags": [
    {%- for tag in site.tags -%}
      {{- tag | first | jsonify -}}{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ],
  "categories": [
    {%- for category in site.categories -%}
      {{- category | first | jsonify -}}{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ],
  "posts": [
    {%- for post in site.posts -%}
      {
        "title": {{ post.title | jsonify }},
        "url": {{ post.relative_path | jsonify }},
        "date": {{ post.date | date_to_xmlschema | jsonify }}
      }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
}
